{
    "name": "generic-lang",
    "displayName": "generic Language",
    "description": "Syntax highlighting for generic-lang",
    "version": "0.0.6",
    "publisher": "JanEricNitschke",
    "author": "JanEricNitschke",
    "icon": "images/CILogo.png",
    "engines": {
        "vscode": "^1.24.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "generic-lang",
                "aliases": [
                    "generic-lang"
                ],
                "extensions": [
                    ".gen"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "generic-lang",
                "scopeName": "source.generic-lang",
                "path": "./syntaxes/generic-lang.tmLanguage.json"
            }
        ]
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/JanEricNitschke/generic-lang.git"
    },
    "keywords": [
        "generic-lang",
        "vscode",
        "vscode-extension",
        "vscode-language"
    ],
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/JanEricNitschke/generic-lang/issues"
    },
    "homepage": "https://github.com/JanEricNitschke/generic-lang#readme"
}
